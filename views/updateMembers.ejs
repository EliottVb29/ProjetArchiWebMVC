        <label for="lposition">Position :</label>
        <input id="lposition" type="text" name="position" value="<%= position %>"> <br>
        <label for="lstatus">Status :</label>
        <input id="lstatus" type="text" name="status" value="<%= status %>"> <br>
        <input type="submit" name="save" value="Sauver"><br>
    </form>

